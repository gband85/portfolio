{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "Web portfolio built with webpack",
  "scripts": {
    "build:dev": "webpack --mode=development --watch --progress",
    "build:prod": "webpack --mode=production",
    "prepublishOnly": "webpack --mode=production",
    "lint": "eslint \"{src,apps,libs,test}/**/*.js\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.js\" --fix",
    "format": "prettier --write \"src/**/*.js\"",
    "preinstall": "npx npm-force-resolutions"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gband85/portfolio.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gband85/portfolio/issues"
  },
  "homepage": "https://github.com/gband85/portfolio#readme",
  "devDependencies": {
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "html-loader": "^3.1.2",
    "html-webpack-plugin": "^5.5.0",
    "prettier": "^2.7.1",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  },
